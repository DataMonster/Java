package collection3;

public class FirstMissingPositive {

	public static void pre (int n){
		int result = 1;
		for(int i=2;i<=n;i++){
			result|=i;
		}
		System.out.println(result);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=2;i<12;i++){pre(i);}
	}

}
