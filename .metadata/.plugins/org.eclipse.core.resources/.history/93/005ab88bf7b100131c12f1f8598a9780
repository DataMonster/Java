package edu.java.moderate;

/*
 * write a function to swap a number in place, that is, without temporary variables.
 */
public class SwapNumber {
	static int x;
	static int y;
	
	public static void swap(){
		x = (x|y)&y;
		y = (x|y)&x;
	}
	
	public static void main(String[] args) {
		x = 9;
		y = 25;
		
	}

}
